datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Update {
  id          Int       @id @default(autoincrement())
  time        DateTime
  reportId    Int
  residentsId Int?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  deletedAt   DateTime?
  complaints  Json?
}
