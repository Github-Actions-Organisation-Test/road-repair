datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Admin {
  id              Int        @id @default(autoincrement())
  mayorId         Int
  userId          Int
  createdAt       DateTime   @default(now())
  updatedAt       DateTime   @updatedAt
  deletedAt       DateTime?
  supervisors     Json?
  resources       Resource[]
  repairSchedules Json?
}

model Resource {
  id            Int       @id @default(autoincrement())
  name          String
  quantity      Int
  allocationId  Int
  price         Int
  supervisorsId Int?
  reportId      Int?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
  deletedAt     DateTime?
  admin         Admin?    @relation(fields: [adminId], references: [id])
  adminId       Int?
}
