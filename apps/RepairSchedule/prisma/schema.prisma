datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model RepairSchedule {
  id          Int       @id @default(autoincrement())
  time        DateTime
  admin       Admin?    @relation(fields: [AdminId], references: [id])
  AdminId     Int?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  deletedAt   DateTime?
  supervisors Json?
  repairs     Json?
  report      Json?
}

model Admin {
  id              Int              @id @default(autoincrement())
  mayorId         Int
  userId          Int
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  deletedAt       DateTime?
  supervisors     Json?
  resources       Json?
  repairSchedules RepairSchedule[]
}
